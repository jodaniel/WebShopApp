@using WMarket.Models
@{
    ViewBag.Title = "Index";
    MensajeError mError = ViewData["mError"] as MensajeError;
}
<div id="Error">
    <h2>Error: @mError.Database</h2>
    <h4>Mensaje: @mError.Mensaje</h4>

    @using (Html.BeginForm("ErrorVolver", "Error", FormMethod.Post))
    {

        <form method="POST">
            @Html.ActionLink("Volver", "ErrorVolver", mError)
        </form>
    }
</div>

